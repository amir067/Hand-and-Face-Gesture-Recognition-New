<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt"
    android:color="#E91E63">
    <item>

        <shape android:shape="ring" >

            <padding android:padding="5dp" />



            <gradient android:startColor="#00EEF3EA"
                android:endColor="#00EEF3EA"
                android:angle="0"
                android:centerColor="#00EEF3EA"/>
            <corners android:radius="45dp"/>
        </shape>
    </item>

    <item android:id="@android:id/mask">

        <shape android:shape="rectangle" >

            <padding android:padding="5dp" />


            <solid android:color="#E91E63" />
            <corners android:radius="45dp"/>
        </shape>
    </item>
</ripple>
